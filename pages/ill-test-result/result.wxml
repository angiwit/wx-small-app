<view class="page">
  <view class="page__hd">
    <!-- <view class="page__title">Article</view>
        <view class="page__desc">文章</view> -->
  </view>
  <view class="page__bd">
    <view class="weui-article">
      <view class="weui-article__h1">我的病情结果</view>
      <view class="weui-article__section">
        <view class="weui-article__title">神经病★★★★★</view>
        <view class="weui-article__p">
          <image class="weui-article__img" src="../../images/pic_article.png" mode="aspectFit" style="height: 150px" />
        </view>
        <view class="weui-article__section">
          <view class="weui-article__h3"></view>
          <view class="weui-article__p" wx:if="{{testResult != ''}}">
            {{testResult}}
          </view>
          <view class="weui-article__p" wx:else>
            需要从接口读取数据
          </view>
        </view>
      </view>
    </view>

    <view class="page__bd page__bd_spacing">
        <button class="weui-btn" type="primary" bindtap='healing'>开始治病</button>
        <!-- <button class="weui-btn" type="primary" disabled="true">页面主操作 Disabled</button> -->
    </view>

  </view>
</view>